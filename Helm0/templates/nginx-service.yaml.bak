apiVersion: v1
kind: Service
metadata:
  name: nginx-service
  namespace: default
spec:
  type: NodePort  # or LoadBalancer
  selector:
    app: nginx  # Ensure this matches your NGINX deployment
  ports:
    - protocol: TCP
      port: 443         # Service port for HTTPS
      targetPort: 443   # NGINX container port
      nodePort: 32000   # NodePort for accessing NGINX

